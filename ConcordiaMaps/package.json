    "@react-native-community/cli": "^15.1.3",
    "@testing-library/jest-native": "^5.0.0",
    "@testing-library/react-native": "^13.0.1",
    "@types/react": "~18.3.12",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
