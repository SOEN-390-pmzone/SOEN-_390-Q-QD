{
  "expo": {
    "name": "ConcordiaMaps",
    "slug": "ConcordiaMaps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "Allow ConcordiaMaps to access your calendar.",
        "NSRemindersUsageDescription": "Allow ConcordiaMaps to access your reminders."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ],
      "package": "com.anonymous.ConcordiaMaps"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-calendar",
        {
          "calendarPermission": "Allow ConcordiaMaps to access your calendar.",
          "remindersPermission": "Allow ConcordiaMaps to access your reminders."
        }
      ]
    ]
  }
}
